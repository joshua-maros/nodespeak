Float twelve = 12.0, thirteen = 13.0;
assert(twelve == 12.0);
assert(thirteen == 13.0);
assert(twelve + thirteen == 25.0);
assert(thirteen - twelve == 1.0);
assert(twelve * thirteen == 156.0);
assert(thirteen % twelve == 1.0);

input Float value1, value2;
output Float sum, difference, product, remainder;
sum = value1 + value2;
difference = value1 - value2;
product = value1 * value2;
remainder = value1 % value2;